{
  "name": "personal-website",
  "version": "0.1.0",
  "description": "Personal website of Anton Grabovsky",
  "productName": "Personal website of Anton Grabovsky",
  "author": "Anton Grabovsky <agrabovsky2020@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
